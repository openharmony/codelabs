<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <oatconfig>
        <licensefile>LICENSE</licensefile>
        <policylist>
            <policy name="projectPolicy" desc="">
                <policyitem type="copyright" name="Huawei Device Co., Ltd." path=".*" desc=""/>
                <policyitem type="copyright" name="OpenAtom Foundation" path=".*" desc=""/>
            </policy>
        </policylist>
        <filefilterlist>
            <filefilter name="binaryFileType" desc="Binary files">
                <filteritem type="filename" name="*.jpg"/>
                <filteritem type="filename" name="*.png"/>
                <filteritem type="filename" name="*.gif"/>
                <filteritem type="filename" name="*.mp4"/>
                <filteritem type="filename" name="*.mp3"/>
                <filteritem type="filename" name="*.webp"/>
                <filteritem type="filename" name="*.json"/>
                <filteritem type="filename" name="*.json5"/>
            </filefilter>
            <filefilter name="defaultPolicyFilter" desc="Filters for default policy">
                <filteritem type="filepath" name="build/.*" desc="Build output"/>
                <filteritem type="filepath" name="node_modules/.*" desc="Node modules"/>
            </filefilter>
        </filefilterlist>
    </oatconfig>
</configuration>